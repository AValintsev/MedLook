@model FoxNetSoft.Plugin.Payments.LiqPay.Models.LiqPayGatewayModel
@{
    Layout = "";
}
<script>
    $(function () {
        $("#liqpaysubmit").submit();
    });
</script>
<div style="text-align: center;">
    <div id="wmheader">
        Переход в LiqPay Gateway
    </div>
    <form id="liqpaysubmit" method="POST" action="https://www.liqpay.ua/api/checkout" accept-charset="utf-8">
        <input type="hidden" name="data" value="@Model.Data" />
        <input type="hidden" name="signature" value="@Model.Signature" />
        <input type="image" src="//static.liqpay.ua/buttons/p1ru.radius.png" />
    </form>
</div>